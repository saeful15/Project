void panggil_cetak(Stock s[],int w,int f){
	clrscrr();
   if(f>20)
   lihat_barang(21);
   else
   lihat_barang(f+1);
	cetak_barang(s,w-1,f);
	char pilih[2];
   gotoxy(54,16);printf("Masukan Pilihan : ");gets(pilih);
   if(strcmp(pilih,"3")==0 && w!=1){
   	panggil_cetak(s,w-1,f-20);
   }
   else if(strcmp(pilih,"4")==0 && w!=nmr){
      if(w>1)l-=40;
   	else l-=20;
   	panggil_cetak(s,w+1,f+20);
   }
   else if(strcmp(pilih,"1")==0){
   	l=0;
      nmr=0;
   	if(strcmp(kunci,"1")==0){
      	tampilan_admin_up_copy();
      	tampilan_admin_down();
      }
      else if(strcmp(kunci,"2")==0){
      	tampilan_admin_up_copy();
      	tampilan_user_down();
      }
   }
   else if(strcmp(pilih,"2")==0){
   	menu_cari();
   }
}
